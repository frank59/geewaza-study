<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geewaza.springmvc.dao.LoginLogDao">
    <insert id="insertLoginLog" parameterType="com.geewaza.springmvc.domain.LoginLog">
       INSERT INTO t_login_log(user_id, ip, login_datetime) VALUES(#{userId},#{ip},#{loginDate})
    </insert>

</mapper>